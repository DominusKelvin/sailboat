{
  "name": "sailboat",
  "displayName": "Sailboat",
  "description": "Sails tooling for vscode.",
  "version": "0.0.1",
  "publisher": "dominuskelvin",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Languages"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DominusKelvin/sailboat"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sailboat.openSailsCoreConcepts",
        "title": "Sailboat: Open Sails Core Concepts"
      },
      {
        "command": "sailboat.openSailsAPIReference",
        "title": "Sailboat: Open Sails API Reference"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.44.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
